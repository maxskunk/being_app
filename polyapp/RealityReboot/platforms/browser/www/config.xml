<?xml version='1.0' encoding='utf-8'?>
<widget id="com.zokyamedia.realityreboot" version="0.1.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Reality Reboot</name>
    <description>
            Reality Reboot
        </description>
    <author email="info@zokyamedia.com" href="http://zokyamedia.com">
            Zokya Media
        </author>
    <icon src="res/icon_72.png" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="orientation" value="portrait" />
    <preference name="FullScreen" value="true" />
</widget>
